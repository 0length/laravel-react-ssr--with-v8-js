        "jest": "^25.2.3",
        "jest-junit": "^10.0.0",
        "mocha": "^5.2.0",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.1",
        "typescript": "^3.8.3",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-node-externals": "^1.7.2",
